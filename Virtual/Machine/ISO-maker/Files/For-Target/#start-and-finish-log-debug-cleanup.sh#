#!/bin/bash

grep -v 'Setting up' start-and-finish.log | grep -v debconf | grep -iv generating | grep -v 'Get:' | grep -v 'Selecting ' | grep -v Unpacking | grep -v Reading | grep -v found | grep -v Adding | grep -v Extracting | grep -v Preparing | grep -v Fetched | grep -v Created | grep -v Processing | grep -v Compiling | grep -v Checking | grep -v Parsing | grep -v Wrote | grep -v 'is already the newest' | grep -v 'set to manually ' | grep -v Importing | grep -v 'lexical' | grep -v 'free variable' | grep -v 'update-alternatives' | grep -v ': processing:' | grep -v 'Updating PPD' | grep -v 'stable command' | fgrep -v '    - ' | grep -v 'no change' | grep -v InstallContrib | grep -v 'pkgs/main' | grep -v 'Requirement already satisfied' | grep -v 'Resolving ' | grep -v 'Connected.' 
